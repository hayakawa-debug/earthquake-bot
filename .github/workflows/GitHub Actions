on:
  schedule:
    - cron: "*/5 * * * *"  # 5分ごとに実行
  workflow_dispatch:

jobs:
  run:
    runs-on: ubuntu-latest
    steps:
      - uses: actions/checkout@v3
      - uses: actions/setup-python@v4
        with:
          python-version: 3.11
      - run: pip install feedparser
      - run: python bot.py
